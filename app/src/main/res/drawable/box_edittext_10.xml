<?xml version="1.0" encoding="utf-8"?>
<inset xmlns:android="http://schemas.android.com/apk/res/android">
    <selector>
        <item android:state_enabled="false" >
            <layer-list>
                <item>
                    <shape android:shape="rectangle" android:useLevel="true">
                        <solid android:color="@color/white_10"/>
                        <corners android:radius="50dp"/>
                    </shape>
                </item>
                <item android:left="48dp" android:right="24dp" android:bottom="5dp">
                    <nine-patch android:src="@drawable/abc_textfield_default_mtrl_alpha"/>
                </item>
            </layer-list>
        </item>
        <item android:state_pressed="false" android:state_focused="false" >
            <layer-list>
                <item>
                    <shape android:shape="rectangle" android:useLevel="true">
                        <solid android:color="@color/white_10"/>
                        <corners android:radius="50dp"/>
                    </shape>
                </item>
                <item android:left="48dp" android:right="24dp" android:bottom="5dp">
                    <nine-patch android:src="@drawable/abc_textfield_default_mtrl_alpha"/>
                </item>
            </layer-list>
        </item>
        <item >
            <layer-list>
                <item>
                    <shape android:shape="rectangle" android:useLevel="true">
                        <solid android:color="@color/white_10" />
                        <corners android:radius="50dp"/>
                    </shape>
                </item>
                <item android:left="48dp" android:right="24dp" android:bottom="5dp">
                    <nine-patch android:src="@drawable/abc_textfield_activated_mtrl_alpha"/>
                </item>
            </layer-list>
        </item>
    </selector>
</inset>